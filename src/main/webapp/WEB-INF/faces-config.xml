<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
    version="2.3">
	<application>
		<locale-config>
			<default-locale>en</default-locale>
			<supported-locale>ar</supported-locale>
		</locale-config>
		<resource-bundle>
			<base-name>localize.messages</base-name>
			<var>msg</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>application-local</base-name>
			<var>properties</var>
		</resource-bundle>
	</application>
	<managed-bean>
		<managed-bean-name>sectorBean</managed-bean-name>
		<managed-bean-class>com.tawasul.web.beans.SectorBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
		<managed-property>
			<property-name>sectorService</property-name>
			<property-class>com.tawasul.web.service.SectorService</property-class>
			<value>#{sectorService}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>consultationBean</managed-bean-name>
		<managed-bean-class>com.tawasul.web.beans.ConsultationBean</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
		<managed-property>
			<property-name>consultationService</property-name>
			<property-class>com.tawasul.web.service.ConsultationService</property-class>
			<value>#{sectorService}</value>
		</managed-property>
	</managed-bean>
</faces-config>
